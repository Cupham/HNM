		<!--Set CurrentDateSetting-->
		<action>
			<name>SetCurrentDateSetting</name>
			<argumentList>
				<name>NewCurrentDateSetting</name>
				<direction>in</direction>
				<relatedStateVariable>CurrentDateSetting</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get PowerLimit-->
		<action>
			<name>GetPowerLimit</name>
			<argumentList>
				<name>CurrentPowerLimit</name>
				<direction>out</direction>
