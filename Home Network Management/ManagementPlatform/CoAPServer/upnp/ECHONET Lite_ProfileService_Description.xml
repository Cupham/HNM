		<!--Get CumulativePower-->
		<action>
			<name>GetCumulativePower</name>
			<argumentList>
				<name>CurrentCumulativePower</name>
				<direction>out</direction>
				<relatedStateVariable>CumulativePower</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get ManufactureerFaultCode-->
		<action>
			<name>GetManufactureerFaultCode</name>
			<argumentList>
				<name>CurrentManufactureerFaultCode</name>
				<direction>out</direction>
				<relatedStateVariable>ManufactureerFaultCode</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get CurrentLimitSetting-->
		<action>
			<name>GetCurrentLimitSetting</name>
			<argumentList>
				<name>CurrentCurrentLimitSetting</name>
				<direction>out</direction>
				<relatedStateVariable>CurrentLimitSetting</relatedStateVariable>
			</argumentList>
		</action>
		<!--Set CurrentLimitSetting-->
		<action>
			<name>SetCurrentLimitSetting</name>
			<argumentList>
				<name>NewCurrentLimitSetting</name>
				<direction>in</direction>
				<relatedStateVariable>CurrentLimitSetting</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get FaultStatus-->
		<action>
			<name>GetFaultStatus</name>
			<argumentList>
