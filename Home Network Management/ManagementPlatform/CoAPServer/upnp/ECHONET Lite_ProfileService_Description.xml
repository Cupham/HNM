			<argumentList>
				<name>CurrentFaultDescription</name>
				<direction>out</direction>
				<relatedStateVariable>FaultDescription</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get ManufacturerCode-->
		<action>
			<name>GetManufacturerCode</name>
			<argumentList>
				<name>CurrentManufacturerCode</name>
				<direction>out</direction>
				<relatedStateVariable>ManufacturerCode</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get BusinessFacilityCode-->
		<action>
			<name>GetBusinessFacilityCode</name>
			<argumentList>
				<name>CurrentBusinessFacilityCode</name>
				<direction>out</direction>
				<relatedStateVariable>BusinessFacilityCode</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get ProductCode-->
		<action>
			<name>GetProductCode</name>
			<argumentList>
				<name>CurrentProductCode</name>
				<direction>out</direction>
				<relatedStateVariable>ProductCode</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get ProductionNumber-->
		<action>
			<name>GetProductionNumber</name>
			<argumentList>
				<name>CurrentProductionNumber</name>
				<direction>out</direction>
				<relatedStateVariable>ProductionNumber</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get ProductionDate-->
		<action>
			<name>GetProductionDate</name>
			<argumentList>
				<name>CurrentProductionDate</name>
				<direction>out</direction>
				<relatedStateVariable>ProductionDate</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get PowerSaving-->
		<action>
			<name>GetPowerSaving</name>
			<argumentList>
				<name>CurrentPowerSaving</name>
				<direction>out</direction>
				<relatedStateVariable>PowerSaving</relatedStateVariable>
			</argumentList>
		</action>
		<!--Set PowerSaving-->
		<action>
			<name>SetPowerSaving</name>
			<argumentList>
				<name>NewPowerSaving</name>
				<direction>in</direction>
				<relatedStateVariable>PowerSaving</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get RemoteControlSetting-->
		<action>
			<name>GetThroughPublicNetwork</name>
			<argumentList>
				<name>CurrentThroughPublicNetwork</name>
				<direction>out</direction>
				<relatedStateVariable>ThroughPublicNetwork</relatedStateVariable>
			</argumentList>
		</action>
		<!--Set RemoteControlSetting-->
		<action>
			<name>SetThroughPublicNetwork</name>
			<argumentList>
				<name>NewThroughPublicNetwork</name>
				<direction>in</direction>
				<relatedStateVariable>ThroughPublicNetwork</relatedStateVariable>
			</argumentList>
		</action>
		<!--Get CurrentTimeSetting-->
		<action>
			<name>GetCurrentTimeSetting</name>
			<argumentList>
				<name>CurrentCurrentTimeSetting</name>
				<direction>out</direction>
