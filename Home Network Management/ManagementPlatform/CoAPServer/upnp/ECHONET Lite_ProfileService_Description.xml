				<direction>out</direction>
				<relatedStateVariable>CumulativeTime</relatedStateVariable>
			</argumentList>
		</action>
	</actionList>
	<serviceStateTable>
		<!--OperationStatus-->
		<stateVariable sendEvents="yes">
			<name>OperationStatus</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>ON</allowedValue>
				<allowedValue>OFF</allowedValue>
			</allowedValueList>
		</stateVariable>
		<!--InstallationLocation-->
		<stateVariable sendEvents="yes">
			<name>InstallationLocation</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>Living room</allowedValue>
				<allowedValue>Dining room</allowedValue>
				<allowedValue>Kitchen</allowedValue>
				<allowedValue>Bathroom</allowedValue>
				<allowedValue>Lavatory</allowedValue>
				<allowedValue>Washroom/changing room</allowedValue>
				<allowedValue>Passageway</allowedValue>
				<allowedValue>Room</allowedValue>
				<allowedValue>Stairway</allowedValue>
				<allowedValue>Front door</allowedValue>
				<allowedValue>Storeroom</allowedValue>
				<allowedValue>Garden/perimeter</allowedValue>
				<allowedValue>Garage</allowedValue>
				<allowedValue>Veranda/balcony</allowedValue>
				<allowedValue>Others</allowedValue>
			</allowedValueList>
		</stateVariable>
		<!--StandardVersionInfo-->
		<stateVariable sendEvents="no">
			<name>StandardVersionInfo</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--IdentificationNumber-->
		<stateVariable sendEvents="no">
			<name>IdentificationNumber</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--InstantaneousPower-->
		<stateVariable sendEvents="no">
			<name>InstantaneousPower</name>
			<dataType>ui1</dataType>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>65533</maximum>
				<step>1</step>
			</allowedValueRange>
		</stateVariable>
		<!--CumulativePower-->
		<stateVariable sendEvents="no">
			<name>CumulativePower</name>
			<dataType>float</dataType>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>999999.999</maximum>
				<step>0.001</step>
			</allowedValueRange>
		</stateVariable>
		<!--ManufactureerFaultCode-->
		<stateVariable sendEvents="no">
			<name>ManufactureerFaultCode</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--CurrentLimitSetting-->
		<stateVariable sendEvents="no">
			<name>CurrentLimitSetting</name>
			<dataType>ui1</dataType>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>100</maximum>
				<step>1</step>
			</allowedValueRange>
		</stateVariable>
		<!--FaultStatus-->
		<stateVariable sendEvents="yes">
			<name>FaultStatus</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>true</allowedValue>
				<allowedValue>false</allowedValue>
			</allowedValueList>
		</stateVariable>
		<!--FaultDescription-->
		<stateVariable sendEvents="">
			<name>FaultDescription</name>
			<dataType>string</dataType>
			<defaultValue>No fault</defaultValue>
		</stateVariable>
		<!--ManufacturerCode-->
		<stateVariable sendEvents="no">
			<name>ManufacturerCode</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--BusinessFacilityCode-->
		<stateVariable sendEvents="no">
			<name>BusinessFacilityCode</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--ProductCode-->
		<stateVariable sendEvents="no">
			<name>ProductCode</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--ProductionNumber-->
		<stateVariable sendEvents="no">
			<name>ProductionNumber</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--ProductDate-->
		<stateVariable sendEvents="no">
			<name>ProductDate</name>
			<dataType>Date</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--PowerSaving-->
		<stateVariable sendEvents="no">
			<name>PowerSaving</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>ON</allowedValue>
				<allowedValue>OFF</allowedValue>
			</allowedValueList>
		</stateVariable>
		<!--RemoteControlSetting-->
		<stateVariable sendEvents="no">
			<name>ThroughPublicNetwork</name>
			<dataType>string</dataType>
			<allowedValueList>
				<allowedValue>YES</allowedValue>
				<allowedValue>NO</allowedValue>
			</allowedValueList>
		</stateVariable>
		<!--CurrentTimeSetting-->
		<stateVariable sendEvents="no">
			<name>CurrentTimeSetting</name>
			<dataType>Time</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--CurrentDateSetting-->
		<stateVariable sendEvents="no">
			<name>CurrentDateSetting</name>
			<dataType>Date</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<!--PowerLimit-->
		<stateVariable sendEvents="no">
			<name>PowerLimit</name>
			<dataType>ui1</dataType>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>65535</maximum>
				<step>1</step>
			</allowedValueRange>
		</stateVariable>
		<!--CumulativeTime-->
		<stateVariable sendEvents="no">
			<name>CumulativeTime</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
	</serviceStateTable>
</scpd>
